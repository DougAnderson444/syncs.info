{
  "name": "syncs-info",
  "description": "Sync your info amongst peers",
  "version": "0.0.1",
  "scripts": {
    "vercel dev": "vercel dev",
    "dev": "sapper dev --port 3000",
    "build": "sapper export",
    "export": "sapper export",
    "start": "vercel link VERCEL_ORG_ID=$VERCEL_ORG_ID VERCEL_PROJECT_ID=$VERCEL_PROJECT_ID --confirm --token $VERCEL_TOKEN && vercel dev --listen 3000",
    "cy:open": "cypress open",
    "start:ci": "start-server-and-test start http://localhost:3000 test",
    "test": "run-p --race dev cy:run",
    "cy:run": "npx cypress run --record --key d5d0c4ea-23ad-420f-8f77-7c8447a1d338"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cypress": "^4.11.0",
    "faunadb": "^2.14.2",
    "ipfs": "^0.48.1",
    "ipfs-http-client": "^44.0.0",
    "normalize-value": "^1.0.1",
    "polka": "next",
    "prom-client": "^12.0.0",
    "sapper-environment": "^1.0.1",
    "sirv": "^0.4.0",
    "uid-promise": "^1.1.0",
    "zxcvbn": "^4.4.2",
    "streamlined-idm-wallet-sdk": "git://github.com/DougAnderson444/streamlined-idm-wallet-sdk.git#13cbf3e098ac92fe1e69098abad3197e6b96b7b3"
  },
  "devDependencies": {
    "@smui/button": "^1.0.0-beta.21",
    "@smui/card": "^1.0.0-beta.21",
    "@smui/checkbox": "^1.0.0-beta.21",
    "@smui/common": "^1.0.0-beta.21",
    "@smui/form-field": "^1.0.0-beta.21",
    "@smui/textfield": "^1.0.0-beta.21",
    "chai": "^4.2.0",
    "css-loader": "^4.0.0",
    "cssnano": "^4.1.10",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.0.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sapper": "^0.27.0",
    "sass-loader": "^9.0.2",
    "start-server-and-test": "1.9.0",
    "style-loader": "^1.2.1",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.9.0",
    "webpack": "^4.7.0"
  },
