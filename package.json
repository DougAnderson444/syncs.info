{
  "name": "SyncsInfo",
  "description": "Sync your info amongst peers",
  "version": "0.0.1",
  "scripts": {
    "vercel dev": "vercel dev",
    "dev": "sapper dev --port 3000",
    "build": "sapper export",
    "export": "sapper export SAPPER_TIMESTAMP=$(date +%s%3N)",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "ipfs": "^0.47.0",
    "ipfs-postmsg-proxy": "DougAnderson444/ipfs-postmsg-proxy",
    "polka": "next",
    "sapper-environment": "^1.0.1",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.9.0",
    "webpack": "^4.7.0"
  },
  "repository": "https://github.com/DougAnderson444/syncs.info",
  "license": "GPL-3.0-or-later"
}
