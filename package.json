{
  "name": "syncs-info",
  "description": "Sync your info amongst peers",
  "version": "0.0.1",
  "scripts": {
    "vercel dev": "vercel dev",
    "dev": "sapper dev --port 3000",
    "build": "sapper export",
    "export": "sapper export",
    "start": "vercel -c -d -f --env [NOW_ORG_ID=$NOW_ORG_ID NOW_PROJECT_ID=$NOW_PROJECT_ID] && vercel dev --listen 3000",
    "cy:open": "cypress open",
    "start:ci": "start-server-and-test start http://localhost:3000 test",
    "test": "run-p --race dev cy:run",
    "cy:run": "npx cypress run --record --key d5d0c4ea-23ad-420f-8f77-7c8447a1d338"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "compression": "^1.7.1",
    "cypress": "^4.11.0",
    "faunadb": "^2.14.2",
    "ipfs": "^0.48.1",
    "polka": "next",
    "sapper-environment": "^1.0.1",
    "sirv": "^0.4.0",
    "uid-promise": "^1.1.0"
  },
  "devDependencies": {
    "chai": "*",
    "http-server": "^0.12.3",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "sapper": "^0.27.0",
    "start-server-and-test": "1.9.0",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.9.0",
    "webpack": "^4.7.0"
  },
  "repository": "https://github.com/DougAnderson444/syncs.info",
  "license": "GPL-3.0-or-later"
}
